        $("#profileForm").on('submit',function (e) {
            e.preventDefault();
            if (!$(this).valid()) {
                return;
            }


            console.log(this)
            $("#submitProfileBtn").prop('disabled', true);
            $.blockUI({ message: '<h1 class="text-center"><img src="/SinaCustom/img/Spinner-0.9s-58px.svg" />  </h1>' });
            $.ajax({
                method: "POST",
                url: '/Profiles/Index',
                //data: $('#profileForm').serialize() ,
                data: new FormData(this) ,
                processData: false,
                contentType: false,

                //contentType: "application/json; charset=utf-8",
                success: function (r) {
                 
                    if (r.status) {
                        $("#submitProfileBtn").prop('disabled', false);
                        $.unblockUI();
                        SuccessAlert(r.message)
                        $("#ProfleImageAddress").val(r.data)
                        $("#UploadedImage").attr("src", r.data);

                    }
                    else {
                        $("#submitProfileBtn").prop('disabled', false);
                        $.unblockUI();
                        FailedAlert(r.message);
                    }
                },
